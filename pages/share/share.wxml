<!--pages/share/share.wxml-->
<view class="container">
    <button bindtap="uploadPictures"> + </button>
</view>
<view>
  <view class= "card" wx:for = "{{items}}">
    <view data-url="{{item.location}}">
       <view class="title">{{item.user_ID}}</view>
       <image class= "image" mode="aspectFill" src="{{item.location}}"/>
    </view>
  </view>
</view>
